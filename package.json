{
  "name": "@cludz/sdk",
  "version": "0.2.6",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "dev": "tsup index.ts --format cjs,esm --watch --dts",
    "lint": "bun x eslint ."
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "description": "Cludz API JavaScript SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/mee-i/cludz-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/mee-i/cludz-sdk/issues"
  },
  "homepage": "https://github.com/mee-i/cludz-sdk#readme",
  "private": false,
  "type": "module"
}
